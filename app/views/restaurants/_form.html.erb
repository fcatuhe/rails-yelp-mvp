<%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :category, include_blank: 'Select category', collection: ["chinese", "italian", "japanese", "french", "belgian"] %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.submit 'Add this restaurant', class: "btn btn-primary" %>
<% end %>
